{
  "path_to_lib": "/home/zanardi/Codes/ML/RONEK/ronek/",
  "env": {
    "backend": "numpy",
    "device": "cpu",
    "device_idx": 0,
    "nb_threads": 8,
    "epsilon": 1e-10,
    "floatx": "float64"
  },
  "grids": {
    "t": {
      "start": 1e-9,
      "stop": 1e-1,
      "num": 500
    }
  },
  "system": {
    "name": "TASystem",
    "kwargs": {
      "use_einsum": false,
      "use_factorial": true,
      "use_arrhenius": true
    }
  },
  "param_space": {
    "defined": {
      "cases": {
        "cold_T6000": {"mu": [5e2, 0.05, 1e3], "T": 6e3},
        "cold_T8000": {"mu": [5e2, 0.05, 1e3], "T": 8e3},
        "cold_T10000": {"mu": [5e2, 0.05, 1e3], "T": 1e4},
        "cold_T12000": {"mu": [5e2, 0.05, 1e3], "T": 1.2e4},
        "cold_T14000": {"mu": [5e2, 0.05, 1e3], "T": 1.4e4}
      },
      "mu_type": "mole"
    },
    "sampled": {
      "mu": {
        "limits": {
          "T0": [5e2, 5e3],
          "w0_a": [0.01, 1.0],
          "rho": [1e-4, 1e0]
        },
        "nb_samples": 100,
        "log_vars": ["T0", "rho"],
        "eps": 1e-7
      },
      "T": {
        "limits": [5e3, 1.5e4],
        "nb_samples": 10
      }
    },
    "nb_workers": 10
  },
  "paths": {
    "dtb": "/home/zanardi/Codes/ML/RONEK/ronek/examples/RVC_O3/database/",
    "saving": "./../../test/data/"
  }
}
