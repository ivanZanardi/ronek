{
  "path_to_lib": "/home/zanardi/Codes/ML/RONEK/ronek/",
  "env": {
    "backend": "numpy",
    "device": "cpu",
    "device_idx": 0,
    "nb_threads": 8,
    "epsilon": 1e-10,
    "floatx": "float64"
  },
  "grids": {
    "t": {
      "start": 1e-9,
      "stop": 1e-3,
      "num": 500
    }
  },
  "system": {
    "name": "TASystem",
    "kwargs": {
      "T": 1e4,
      "use_einsum": false,
      "use_factorial": false
    }
  },
  "param_space": {
    "defined": {
      "cold": [5e2, 1e3, 0.05],
      "hot":  [4e3, 1e3, 0.05]
    },
    "sampled": {
      "T_lim": [5e2, 1e4],
      "p_lim": [5e2, 5e4],
      "X_a_lim": [0.05, 0.95],
      "nb_samples": 100
    },
    "nb_workers": 20
  },
  "paths": {
    "dtb": "./../database/",
    "saving": "./test01/"
  }
}
