{
  "path_to_lib": "/home/zanardi/Codes/ML/RONEK/ronek/",
  "env": {
    "backend": "numpy",
    "device": "cpu",
    "device_idx": 0,
    "nb_threads": 4,
    "epsilon": 1e-10,
    "floatx": "float64"
  },
  "grids": {
    "t": {
      "start": 1e-9,
      "stop": 1e-3,
      "num": 500
    }
  },
  "system": {
    "name": "TAFASystem",
    "kwargs": {
      "T": 1e4,
      "use_einsum": false,
      "use_factorial": true
    }
  },
  "param_space": {
    "defined": {
      "cases": {
        "cold": [5e2, 0.05, 1e3],
        "hot":  [4e3, 0.05, 1e3]
      },
      "mu_type": "mole"
    },
    "sampled": {
      "T_lim": [5e2, 1e4],
      "w_a_lim": [0.01, 0.99],
      "rho_lim": [1e-4, 1e0],
      "nb_samples": 1000
    },
    "nb_workers": 10
  },
  "paths": {
    "dtb": "./../../database/",
    "saving": "./../../test/data/"
  }
}
